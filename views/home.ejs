<%- include("./partials/header") -%> 
    <h1>Home</h1>
    <p><%= homeStartingContent %> </p>

    <% posts.forEach(e => { %>
        <h1><%= e.title %> </h1>
        <p>
            <%= `${e.message.substring(0,100)}...` %> 
            <a href="/post/<%= e.title %> ">Read More</a>
        </p>
    <% }) %>
<%- include("./partials/footer") -%> 