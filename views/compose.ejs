<%- include("./partials/header") -%> 
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="titleMessage">Title</label>
            <input type="text" class="form-control" id="titleMessage" name="titleMessage">
            <label for="postMessage">Post</label>
            <textarea class="form-control" id="postMessage" name="postMessage" cols="5" rows="10"></textarea>
        </div>
        <button type="submit" class="btn btn-primary" name="button">Submit</button>
    </form>
<%- include("./partials/footer") -%> 